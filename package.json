{
  "name": "lottie",
  "version": "1.0.0",
  "description": "Default buildchain - yarn, webpack, ES6, SASS",
  "main": "dist/app.js",
  "author": "Name <email>",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
